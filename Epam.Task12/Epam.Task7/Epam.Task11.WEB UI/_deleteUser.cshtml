@using System;
@using Epam.Task7.Users.BLL.Interface;
@using Epam.Task7.Users.Common;
@using Epam.Task7.Users.Entities;
@if (IsPost)
{
    var userLogic = DependencyResolver.UserLogic;
    string idUser = Request.Params["iduser"] ?? string.Empty;
    int.TryParse(idUser, out int userdelete);
    userLogic.DeleteUser(userdelete);
    Response.Redirect("/AllUsers");

}