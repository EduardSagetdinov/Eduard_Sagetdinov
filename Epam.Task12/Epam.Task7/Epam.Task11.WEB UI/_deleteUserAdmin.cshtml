@using System;
@using Epam.Task7.Users.BLL.Interface;
@using Epam.Task7.Users.Common;
@using Epam.Task7.Users.Entities;
@if (IsPost)
{
    var userAdminLogic = DependencyResolver.UserAdminLogic;
    string userLogin = Request.Params["userLogin"].Trim(' ') ?? string.Empty;

    userAdminLogic.DeleteUserAdmin(userLogin);


    Response.Redirect("~/AllUserAdmin");

}
